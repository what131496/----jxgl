<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
         "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gsau.entity.Course_score" table="course_score">
	    <composite-id>
	    	<key-property name="stu_id" column="stu_id" type="string" length="12"></key-property>
	    	<key-property name="cour_id" column="cour_id" type="string" length="10"></key-property>
	    </composite-id>
		<property name="score" column="score" type="float" not-null="false"/>
	</class>
</hibernate-mapping>