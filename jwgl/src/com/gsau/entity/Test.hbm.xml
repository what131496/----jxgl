<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gsau.entity.Test" table="test">
		<id name="id" column="id" type="string" length="12">
		   <generator class="assigned"></generator>
		</id>
		<property name="name" column="name" type="string"  length="30" not-null="true"/>
		<property name="pwd" column="pwd" type="string"  length="30"/>
	</class>
</hibernate-mapping>